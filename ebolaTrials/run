#!/usr/bin/env bash

for ((i=0; i < 10; i++))
do
    cd ebola$i/RAxML
    rm *
    cp ../trueData/ebolaTrueMSA.fasta .
    raxml-ng --msa ebolaTrueMSA.fasta --model GTR+FO+I+R4

    cd ../FastTree/RAxML
    rm *
    cp ../../../trueData/ebolaTrueMSA.fasta .
    cat ../fast.tre | resolve_random.py > resolved.tre
    raxml-ng --msa ebolaTrueMSA.fasta --model GTR+FO+I+R4 --evaluate --tree resolved.tre

    cd ../../ClustalOmega/RAxML
    rm *
    cp ../Clustal.aln .
    raxml-ng --msa Clustal.aln --model GTR+FO+I+R4

    cd ../../MUSCLE/RAxML
    rm *
    cp ../MUSCLE.aln .
    raxml-ng --msa MUSCLE.aln --model GTR+FO+I+R4

    cd ../../MAFFT/RAxML
    rm *
    cp ../MAFFT.aln .
    raxml-ng --msa MAFFT.aln --model GTR+FO+I+R4

    cd ../../../
done
